<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="management.perpustakaan.MemberController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label text="Daftar Anggota" style="-fx-font-size: 20px;"/>
    <TableView fx:id="memberTable" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="memberIdColumn" text="ID Anggota" prefWidth="150"/>
            <TableColumn fx:id="nameColumn" text="Nama" prefWidth="250"/>
        </columns>
    </TableView>
    <Button text="Kembali ke Menu Utama" onAction="#handleBackButton"/>
</VBox>